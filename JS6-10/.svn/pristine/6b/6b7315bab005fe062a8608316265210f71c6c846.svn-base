/* 以下是搜索区域自定义css样式 */
.dateWrap {
    border: 1.2px solid #eee;
    margin-bottom: 24px;
    background-color: #fff;
}

.wrapTop,
.wrapBottom {
    display: flex;
    justify-content: space-between;
    padding: 24px 20px;
}

